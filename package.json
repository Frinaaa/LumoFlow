{
  "name": "lumoflow",
  "version": "0.0.1",
  "main": "electron/main.js",
  "scripts": {
    "start": "npm run electron-dev",
    "dev": "vite",
    "build": "tsc && vite build",
    "electron-dev": "concurrently -k \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "electron-prod": "cross-env NODE_ENV=production electron .",
    "build:win": "npm run build && electron-builder --win --publish never",
    "lint": "eslint ."
  },
  "dependencies": {
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^17.2.3",
    "electron-is-dev": "^3.0.0",
    "google-auth-library": "^10.5.0",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^9.1.3",
    "nodemailer": "^7.0.12",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.12.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^25.0.6",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^39.2.7",
    "electron-builder": "^25.0.0",
    "eslint": "^8.19.0",
    "typescript": "^5.8.3",
    "vite": "^7.3.1",
    "wait-on": "^7.2.0"
  }
}
